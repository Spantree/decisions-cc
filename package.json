{
  "name": "decisionapp",
  "version": "0.1.0",
  "description": "Pugh decision matrix React component for weighted multi-criteria evaluation",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "style": "dist/index.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/index.css"
  },
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "demo": "vite",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Spantree/decisionapp.git"
  },
  "keywords": [
    "pugh-matrix",
    "decision-matrix",
    "react",
    "comparison",
    "weighted-scoring"
  ],
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "vite": "^7.3.1"
  },
  "license": "MIT"
}
